{% extends 'base3.html.twig' %}

{% block title %}Registrar Tipo de Aporte{% endblock %}

{% block body %}
    <div class="row page-titles">
        <div class="col-md-5 col-8 align-self-center">
            <h3 class="text-themecolor">Registrar Tipo Aporte</h3>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Admin</a></li>
                <li class="breadcrumb-item active">Registrar Tipo Aporte</li>
            </ol>
        </div>
    </div>
    <div class="row">
        <!-- column -->
        <div class="col-lg-7">
            <div class="card">
                <div class="card-block">
                    <div class="">
                        <form method="post" role="form" name="tipo_aporte">
                            <div class="form-group">
                                <label for="nombre">Nombre Tipo de Aporte</label>
                                <input class="form-control" name="nombre" id="nombre" required>
                                <p class="help-block">Ej. Economico</p>
                            </div>
                            <button type="submit" class="btn waves-effect waves-light btn-warning hidden-md-down">Registrar</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}