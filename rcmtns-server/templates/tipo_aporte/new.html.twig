{% extends 'base2.html.twig' %}

{% block title %}Registrar Tipo de Aporte{% endblock %}

{% block body %}
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-8">
                <h1 class="page-header">Registrar Tipo de Aporte</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <div class="row">
            <div class="col-lg-8">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Registro
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-6">
                                <form method="post" role="form" name="tipo_aporte">
                                    <div class="form-group">
                                        <label for="nombre">Nombre Tipo de Aporte</label>
                                        <input class="form-control" name="nombre" id="nombre" required>
                                        <p class="help-block">Ej. Economico</p>
                                    </div>
                                    <button type="submit" class="btn btn-default">Registrar</button>
                                </form>
                            </div>
                            <!-- /.col-lg-6 (nested) -->
                        </div>
                        <!-- /.row (nested) -->
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
    </div>
{% endblock %}