{% extends 'layout.html.twig' %}

{% block title %}Socio{% endblock %}

{% block body %}
  <h1>Socio</h1>

  <table class="table">
    <tbody>
    <tr>
      <th>Id</th>
      <td>{{ socio.id }}</td>
    </tr>
    <tr>
      <th>Nombre</th>
      <td>{{ socio.nombre }}</td>
    </tr>
    <tr>
      <th>Nit</th>
      <td>{{ socio.nit }}</td>
    </tr>
    <tr>
      <th>Representante</th>
      <td>{{ socio.representante }}</td>
    </tr>
    <tr>
      <th>CedulaRepresentante</th>
      <td>{{ socio.cedulaRepresentante }}</td>
    </tr>
    <tr>
      <th>CorreoRepresentante</th>
      <td>{{ socio.correoRepresentante }}</td>
    </tr>
    <tr>
      <th>TelefonoRepresentante</th>
      <td>{{ socio.telefonoRepresentante }}</td>
    </tr>
    <tr>
      <th>FechaIngreso</th>
      <td>{{ socio.fechaIngreso ? socio.fechaIngreso|date('Y-m-d') : '' }}</td>
    </tr>
    </tbody>
  </table>

  <a href="{{ path('socio_index') }}">back to list</a>

  <a href="{{ path('socio_edit', {'id': socio.id}) }}">edit</a>

  {{ include('socio/_delete_form.html.twig') }}
