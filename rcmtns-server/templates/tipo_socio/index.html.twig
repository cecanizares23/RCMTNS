{% extends 'base.html.twig' %}

{% block title %}TipoSocio index{% endblock %}

{% block body %}
    <h1>TipoSocio index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tipo_socio in tipo_socios %}
            <tr>
                <td>{{ tipo_socio.id }}</td>
                <td>{{ tipo_socio.nombre }}</td>
                <td>
                    <a href="{{ path('tipo_socio_show', {'id': tipo_socio.id}) }}">show</a>
                    <a href="{{ path('tipo_socio_edit', {'id': tipo_socio.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tipo_socio_new') }}">Create new</a>
{% endblock %}